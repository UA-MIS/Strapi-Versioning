{
  "connection": "default",
  "collectionName": "restaurants",
  "info": {
    "name": "restaurants"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "Name": {
      "type": "string",
      "required": true
    },
    "Description": {
      "type": "richtext",
      "required": true
    },
    "Logo": {
      "collection": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "open": {
      "type": "boolean"
    }
  }
}
